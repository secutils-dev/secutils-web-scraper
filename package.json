{
  "name": "secutils-web-scraper",
  "private": true,
  "author": {
    "name": "Secutils.dev",
    "email": "dev@secutils.dev"
  },
  "version": "1.0.0-alpha.6",
  "engines": {
    "node": "20.x"
  },
  "type": "module",
  "main": "dist/src/index.js",
  "scripts": {
    "prebuild": "eslint . && rm -rf dist",
    "build": "tsc --project ./",
    "watch": "SECUTILS_WEB_SCRAPER_ENV_PATH=./.env NODE_OPTIONS=--loader=ts-node/esm nodemon src/index.ts",
    "test": "NODE_NO_WARNINGS=1 node --loader=ts-node/esm --test ./src/tests.ts",
    "test:watch": "NODE_NO_WARNINGS=1 NODE_OPTIONS= node --loader=ts-node/esm --test --watch ./src/tests.ts"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "dependencies": {
    "@fastify/compress": "^7.0.3",
    "dotenv": "^16.4.5",
    "fast-json-stable-stringify": "^2.1.0",
    "fastify": "^4.27.0",
    "js-beautify": "^1.15.1",
    "node-cache": "^5.1.2",
    "playwright": "1.43.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@eslint/eslintrc": "^3.0.2",
    "@types/js-beautify": "^1.14.3",
    "@types/node": "^20.12.10",
    "@typescript-eslint/eslint-plugin": "^7.8.0",
    "@typescript-eslint/parser": "^7.8.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^9.0.11",
    "nodemon": "^3.1.0",
    "pino-pretty": "^11.0.0",
    "prettier": "^3.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5"
  }
}
