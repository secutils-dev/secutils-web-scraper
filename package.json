{
  "name": "secutils-web-scraper",
  "private": true,
  "author": {
    "name": "Secutils.dev",
    "email": "dev@secutils.dev"
  },
  "version": "1.0.0-alpha.4",
  "engines": {
    "node": "20.x"
  },
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "prebuild": "eslint . && rm -rf dist",
    "build": "tsc --project ./",
    "watch": "SECUTILS_WEB_SCRAPER_ENV_PATH=./.env NODE_OPTIONS=--loader=ts-node/esm nodemon src/index.ts",
    "test": "NODE_NO_WARNINGS=1 node --loader=ts-node/esm --test ./src/tests.ts",
    "test:watch": "NODE_NO_WARNINGS=1 NODE_OPTIONS= node --loader=ts-node/esm --test --watch ./src/tests.ts"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "dependencies": {
    "@fastify/compress": "^6.5.0",
    "dotenv": "^16.3.1",
    "fast-json-stable-stringify": "^2.1.0",
    "fastify": "^4.25.1",
    "js-beautify": "^1.14.11",
    "node-cache": "^5.1.2",
    "playwright": "1.40.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.4.3",
    "@commitlint/config-conventional": "^18.4.3",
    "@eslint/eslintrc": "^2.1.4",
    "@types/js-beautify": "^1.14.3",
    "@types/node": "^20.10.4",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.0.1",
    "husky": "^8.0.3",
    "nodemon": "^3.0.2",
    "pino-pretty": "^10.3.0",
    "prettier": "^3.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
